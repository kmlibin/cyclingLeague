    "cookie-parser": "^1.4.6",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "puppeteer": "^21.1.1"
  },
  "devDependencies": {
